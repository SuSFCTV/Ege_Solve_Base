    "def to_num_system(x, system):\n",
    "    numbers = []\n",
    "    while(x > 0):\n",
    "        numbers.append(x % system)\n",
    "        x //= system\n",
    "    return numbers[:-1]\n",
    "\n",
    "print(to_num_system(2 * 216 ** 6 + 3 * 36 ** 9 - 432, 6).count(5))"
   ]
